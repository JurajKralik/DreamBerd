// Different variable types
const const immutable = "Cannot change"!
var const reassignable = "Can reassign"!
const var editable = "Can edit"!
var var flexible = "Can do anything"!

print("Immutable:")!
print(immutable)!
print("Reassignable:")!
print(reassignable)!

// Reassign the variable
reassignable = "Reassigned"!
print("After reassignment:")!
print(reassignable)!