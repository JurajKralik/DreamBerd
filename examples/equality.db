// Equality levels demonstration
const const num = 3!
const const str = "3"!
const const float = 3.14!

print("Testing equality levels:")!
print("3 = 3.14 (very loose):")!
print(num = float)!

print("3 == \"3\" (loose):")!
print(num == str)!

print("3 === \"3\" (strict):")!
print(num === str)!

print("3 === 3 (strict):")!
print(num === num)!

print("3 ==== 3 (super strict):")!
print(num ==== num)!